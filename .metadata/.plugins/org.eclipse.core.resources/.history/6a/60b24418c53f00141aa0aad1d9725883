package com.iepl.pathapp.fragment;

public class Header {
	private  String name;
	private  String description;
	private  int rating;
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @return the description
	 */
	public String getDescription() {
		return description;
	}
	/**
	 * @return the rating
	 */
	public int getRating() {
		return rating;
	}
	
}
